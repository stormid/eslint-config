{
  "name": "@stormid/eslint-config",
  "version": "1.0.0",
  "description": "Storm Id eslint config",
  "main": "index.js",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "eslint index.js",
    "prepare": "npm test",
