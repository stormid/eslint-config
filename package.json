    "extends": "eslint:recommended",
    "env": {
      "node": true
    }
  },
  "author": "stormid",
  "dependencies": {
    "babel-eslint": "^8.2.6",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1"
  }
}
